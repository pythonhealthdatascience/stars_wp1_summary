	month = jul,
	year = {2023},
	pmid = {37433624},
	note = {Publisher: British Medical Journal Publishing Group
Section: Research},
	pages = {e075767},
	file = {Full Text PDF:/home/amy/Zotero/storage/FILBGNV5/Hamilton et al. - 2023 - Prevalence and predictors of data and code sharing.pdf:application/pdf},
}

