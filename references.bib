	journal = {Medical Decision Making},
	author = {Glover, Matthew J. and Jones, Edmund and Masconi, Katya L. and Sweeting, Michael J. and Thompson, Simon G. and Powell, Janet T. and Ulug, Pinar and Bown, Matthew J.},
